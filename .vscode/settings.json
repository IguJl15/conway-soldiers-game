{
  "dart.flutterSdkPath": ".fvm/flutter_sdk",
  // Remove .fvm files from search
  "search.exclude": {
    "**/.fvm": true
  },
  // Remove from file watching
  "files.watcherExclude": {
    "**/.fvm": true
  },
  "spellright.language": [
    "en-US-10-1."
  ],
  "spellright.documentTypes": [
    "markdown",
    "latex",
    "plaintext",
    "dart"
  ],
  "spellright.parserByClass": {
    "dart": {
      "parser": "plain"
    }
  }
}